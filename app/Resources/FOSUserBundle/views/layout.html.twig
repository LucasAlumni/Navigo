<!DOCTYPE html>
<html>
    <head>
        
        <!-- Title -->
        <title>Navigo | Login - Sign in</title>
        
        <meta content="width=device-width, initial-scale=1" name="viewport"/>
        <meta charset="UTF-8">
        <meta name="description" content="Navigo Project Sup'Internet" />
        <meta name="keywords" content="navigo, dashbord" />
        <meta name="author" content="LucasAlumni" />
        
        <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600' rel='stylesheet' type='text/css'>
        <link href="{{ asset ('assets/LoginAPP/plugins/pace-master/themes/blue/pace-theme-flash.css') }}" rel="stylesheet"/>
        <link href="{{ asset ('assets/LoginAPP/plugins/uniform/css/uniform.default.min.css') }}" rel="stylesheet"/>
        <link href="{{ asset ('assets/LoginAPP/plugins/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset ('assets/LoginAPP/plugins/fontawesome/css/font-awesome.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset ('assets/LoginAPP/plugins/line-icons/simple-line-icons.css') }}" rel="stylesheet" type="text/css"/> 
        <link href="{{ asset ('assets/LoginAPP/plugins/waves/waves.min.css') }}" rel="stylesheet" type="text/css"/>  

        <!-- Theme Styles -->
        <link href="{{ asset ('assets/LoginAPP/css/modern.min.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset ('assets/LoginAPP/css/themes/white.css') }}" class="theme-color" rel="stylesheet" type="text/css"/>
        <link href="{{ asset ('assets/LoginAPP/css/custom.css') }}" rel="stylesheet" type="text/css"/>
        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
        
    </head>
    <body class="page-login coming-soon">
        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                {% for message in messages %}
                    <div class="flash-{{ type }}">
                        {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endif %}

        
        {% block fos_user_content %}
        {% endblock fos_user_content %}

        <!-- Javascripts -->
        <script src="{{ asset ('assets/LoginAPP/plugins/jquery/jquery-2.1.3.min.js') }}"></script>
        <script src="{{ asset ('assets/LoginAPP/plugins/jquery-ui/jquery-ui.min.js') }}"></script>
        <script src="{{ asset ('assets/LoginAPP/plugins/bootstrap/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset ('assets/LoginAPP/plugins/jquery-slimscroll/jquery.slimscroll.min.js') }}"></script>
        <script src="{{ asset ('assets/LoginAPP/plugins/uniform/jquery.uniform.min.js') }}"></script>
        <script src="{{ asset ('assets/LoginAPP/plugins/waves/waves.min.js') }}"></script>
        <script src="{{ asset ('assets/LoginAPP/js/modern.min.js') }}"></script>
        
    </body>

    

        
</html>

